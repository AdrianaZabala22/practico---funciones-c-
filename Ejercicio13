#include <iostream>
#include <cmath>
using namespace std;

double redondear(double num, int dec) {
    double p = pow(10, dec);
    return round(num * p) / p;
}

int main() {
    double n; int dec;
    cout << "Ingrese nÃºmero y cantidad de decimales: ";
    cin >> n >> dec;
    cout << "Resultado: " << redondear(n, dec) << endl;
    return 0;
}
